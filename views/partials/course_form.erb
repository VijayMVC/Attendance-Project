<form class="" action="index.html" method="post">
  <div class="form-group">
    <label>Course</label>
    <input type="text" name="name" value="<%= @course.name %>">
  </div>
  <div class="form-group">
    <label>Stream</label>
    <input type="text" name="course_type" value="<%= @course.course_type %>">
  </div>
  <div class="form-group">
    <label>Start Date</label>
    <input type="text" name="start_date" value="<%= @course.start_date %>">
  </div>
  <div class="form-group">
    <label>End Date</label>
    <input type="text" name="end_date" value="<%= @course.end_date %>">
  </div>

  <div class="form-group">
    <input type="submit" name="" value="submit">
  </div>

</form>
